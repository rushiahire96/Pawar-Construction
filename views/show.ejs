<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
  </head>
  <body>
    <div class="container m-10">
        <h1 class="bg-warning p-3 text-center " style="margin-top:20px ;">Bunglo Estimation Design </h1>
        <div class="row bg-light m-10">
            <% data.forEach(function(productObj){ %>
                <div class="col-xl-3 text-center">
                    <!--<img src="<%=productObj.path%>" class="img-fluid"/>-->
                    <img src="/uploads/<%=productObj.path%>" class="img-fluid " style="margin-top:25px ;"/>
                    <h3 class="p-3 text-center " style="margin-top:10px ;">Name:<%=productObj.name%></h3>
                    
                    <h5>Size:<%=productObj.size%></h5>
                    
                    <p>
                        <button class="btn btn-success">Add To Cart</button>
                    </p>
                </div>
            <% }); %>
        </div>
    </div>
   
  </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
  </body>
</html>